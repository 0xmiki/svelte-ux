{
  "name": "svelte-ux",
  "description": "A large collection of Svelte components, actions, stores and utils to simplify creating highly interactive and visual applications.  Built using Tailwind with extensibility and customization in mind.",
  "author": "Sean Lynch <techniq35@gmail.com>",
  "license": "MIT",
  "repository": "techniq/svelte-ux",
  "version": "0.50.0",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-package",
    "prepublishOnly": "svelte-package",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "typedoc": "typedoc",
    "changeset": "changeset",
    "version": "changeset version",
    "version-status": "changeset status -v",
    "publish": "changeset publish"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "^3.0.3",
    "@sveltejs/kit": "^1.25.0",
    "@sveltejs/package": "^2.2.2",
    "@svitejs/changesets-changelog-github-compact": "^1.1.0",
    "@tailwindcss/typography": "^0.5.10",
    "@types/d3-array": "^3.0.7",
    "@types/d3-format": "^3.0.1",
    "@types/d3-scale": "^4.0.4",
    "@types/lodash-es": "^4.17.9",
    "@types/marked": "^5.0.1",
    "@types/prismjs": "^1.26.0",
    "autoprefixer": "^10.4.15",
    "execa": "^8.0.1",
    "marked": "^9.0.0",
    "mdsvex": "^0.11.0",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "sveld": "^0.18.0",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.1",
    "svelte-preprocess": "^5.0.4",
    "svelte2tsx": "^0.6.21",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.2",
    "typedoc-json-parser": "^9.0.0",
    "typescript": "^5.2.2",
    "unist-util-visit": "^5.0.0",
    "vite": "^4.4.9",
    "vitest": "^0.33.0"
  },
  "type": "module",
  "dependencies": {
    "@changesets/cli": "^2.26.2",
    "@floating-ui/dom": "^1.5.2",
    "@fortawesome/fontawesome-common-types": "^6.4.2",
    "@vercel/analytics": "^1.0.2",
    "clsx": "^2.0.0",
    "d3-array": "^3.2.4",
    "d3-format": "^3.1.0",
    "d3-scale": "^4.0.2",
    "d3-time": "^3.1.0",
    "date-fns": "^2.30.0",
    "immer": "^10.0.2",
    "lodash-es": "^4.17.21",
    "rehype-slug": "^6.0.0",
    "tailwind-merge": "^1.14.0",
    "zod": "^3.22.2"
  },
  "peerDependencies": {
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@mdi/js": "^7.0.96",
    "prism-svelte": "^0.5.0",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.29.0",
    "svelte": "^3.56.0 || ^4.0.0",
    "sveld": "^0.18.0"
  },
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "svelte": "./dist/utils/*.js"
    },
    "./plugins/*": "./dist/plugins/*"
  },
  "files": [
    "dist"
  ],
  "svelte": "./dist/index.js"
}
