    "d3-format": "^3.1.0",
    "d3-scale": "^4.0.2",
    "d3-time": "^3.0.0",
    "date-fns": "^2.28.0",
    "graphql": "^16.5.0",
    "immer": "^9.0.15",
    "lodash-es": "^4.17.21",
    "rehype-slug": "^5.0.1",
    "zod": "^3.17.3"
  }
}
